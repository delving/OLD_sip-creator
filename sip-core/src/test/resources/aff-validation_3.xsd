<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.aff-schema.org" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="aff">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="admin">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="ownerUri"/>
              <xs:element type="xs:string" name="ownerName"/>
              <xs:element type="xs:string" name="providerUri"/>
              <xs:element type="xs:string" name="providerName"/>
              <xs:element type="xs:string" name="rigths"/>
              <xs:element type="xs:string" name="checksum"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="collection">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="collectionID"/>
              <xs:element type="xs:string" name="collectionUri"/>
              <xs:element type="xs:string" name="collectionName"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="thing">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="thingId"/>
              <xs:element type="xs:string" name="thingUri"/>
