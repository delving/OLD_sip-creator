              <xs:element type="xs:string" name="displayYear"/>
              <xs:element type="xs:string" name="landingPage"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="mediaFiles">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="mediaFile">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="localURL"/>
                    <xs:element type="xs:string" name="localMediaTypeUri"/>
                    <xs:element type="xs:string" name="localMimeType"/>
                    <xs:element type="xs:string" name="delvingMediaURI"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="actors">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="actor">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="xs:string" name="localId"/>
                    <xs:element type="xs:string" name="localUri"/>
                    <xs:element type="xs:string" name="roleUri"/>
                    <xs:element type="xs:string" name="roleName"/>
                    <xs:element type="xs:string" name="sourceName"/>
                    <xs:element type="xs:string" name="sourceUri"/>
                    <xs:element type="xs:string" name="displayDate"/>
