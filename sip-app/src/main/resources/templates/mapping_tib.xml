<?xml version="1.0"?>
<rec-mapping prefix="tib">

    <!--Copying node mappings to this list will make them appear as candidate mappings when-->
    <!--the prefix corresponds to the target format and the inputPath matches one of the-->
    <!--input paths in the source analysis.-->
    <functions>

    </functions>

    <node-mappings>
        <node-mapping outputPath="/tib:record/tib:material" inputPath="/input/material"/>
        <node-mapping outputPath="/tib:record/dc:description" inputPath="/input/description"/>
        <node-mapping outputPath="/tib:record/tib:creatorDeathYear" inputPath="/input/creator_date_of_death"/>
        <node-mapping outputPath="/tib:record/tib:objectNumber" inputPath="/input/object_number"/>
        <node-mapping outputPath="/tib:record/tib:objectSoort" inputPath="/input/object_name"/>
