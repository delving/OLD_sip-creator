        <node-mapping outputPath="/tib:record/tib:thumbLarge" inputPath="/input/reproduction_identifier_URL"/>
        <node-mapping outputPath="/tib:record/dc:identifier" inputPath="/input/object_number"/>
        <node-mapping outputPath="/tib:record/europeana:uri" inputPath="/input/object_number"/>
