<fact-definition-list>
    <fact-definition name="spec">
        <prompt>Data Set Spec</prompt>
        <toolTip>The reference identifier of this data set</toolTip>
        <automatic>true</automatic>
    </fact-definition>
    <fact-definition name="downloadedSource">
        <prompt>Downloaded Source</prompt>
        <toolTip>This indicates whether the source file is downloaded or imported.</toolTip>
        <automatic>true</automatic>
        <defaultValue>false</defaultValue>
    </fact-definition>
    <fact-definition name="recordRootPath">
        <prompt>Record Path</prompt>
